{
  "compilerOptions": {
    "target": "ES6",
    "allowJs": true,
    "module": "commonjs",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "noImplicitAny": true,
    "sourceMap": true,
    "baseUrl": ".",
    "outDir": "dist",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "paths": {
      "*": ["node_modules/*"],
      "@ssg/*": ["./src/app/*"]
    }
  },
  "include": [
    "src/app/*", "src/modal",
  ],
  "exclude": [
    "src/app/env.example.ts"
  ]
}
